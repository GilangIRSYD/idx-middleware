meta {
  name: Delete Access Token
  type: http
  seq: 7
}

delete {
  url: {{BASE_URL}}{{API_ROUTE}}/config/access-token
  body: none
  auth: inherit
}

headers {
  x-nonce: {{nonce}}
}

settings {
  encodeUrl: true
  timeout: 0
}

docs {
  Delete the access token from memory storage.
  After deletion, the app will fall back to environment variable if available.
  
  Response example:
  {
    "success": true,
    "message": "Access token deleted successfully"
  }
}
